<%= form_for @review do |f| %>
  <%= f.label :rating %>
  <%= f.number_field :rating %><br>
  <%= f.label :content %>
  <%= f.text_area :content %><br>
  <%= f.label :date_visited %>
  <%= f.date_select :date_visited %><br>
  <%= f.label :review_date %>
  <%= f.text_field :review_date %><br>
  <%= f.label :country %>
  <%= f.collection_select(:country_id, @countries, :id, :name)%><br>
  <%= f.collection_select(:city_id, @cities, :id, :name)%><br>
  <%= f.submit %>
<% end %>
